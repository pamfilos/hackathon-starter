div(id="hotel-month-"+hotel["id"])
	each rt in hotelRooms
		div.roomtype-listing
			- for (var i =0 ; i<firstday-1;i++){
				input(name="roomday"+rt.cat+"-"+i, disabled="disabled").roomdateprice.box.NaN
			- }	
			each d in hotelMonth
				input(name="roomday"+rt.cat+"-"+i, value=d.price[rt.cat]['price'], class=d.price[rt.cat]['compare']).roomdateprice.box

	#definite-data
		- for (var i =0 ; i<firstday-1;i++){
			.box.NaN 
		- }	
		each d in hotelMonth
			- if (d.provisional)
				.box #{d.sum - d.provisional}
			- else 
				.box #{d.sum}	
	#provisional-data
		- for (var i =0 ; i<firstday-1;i++){
			.box.NaN 
		- }	
		each d in hotelMonth
			- if (d.provisional)
				.box #{d.provisional}
			- else
				.box 0
	#sum-data
		- for (var i =0 ; i<firstday-1;i++){
			.box.NaN 
		- }	
		each d in hotelMonth
			.box #{d.sum}
	#revcalc-data
		- for (var i =0 ; i<firstday-1;i++){
			.box.NaN 
		- }	
		each d in hotelMonth
			input(value=d.revcal, class=( d.overbooked ? "overbooked " : " ")).box


div(id="calendar-data-"+hotel["id"]).changed

div(id="modified-prices-modal-"+hotel["id"], tabindex="-1",role="dialog",aria-labelledby="pamf",aria-hidden="true").modal.fade.calendar-modal
	div.modal-dialog
		div.modal-content
			div.modal-header
				button(type="button", data-dismiss="modal", aria-label="Close")
					span(arira-hidden="true")
						| &times;
				span(id="myModalLabel").modal-title
					| This is the title

			div.modal-body.row
					
